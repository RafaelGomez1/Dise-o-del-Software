<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.joans.timetracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/hourglasss"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".LlistaActivitatsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LlistaIntervalsActivity" />

        <service
            android:name=".GestorArbreActivitats"
            android:enabled="true" />

        <activity android:name=".newAction" />
        <activity android:name=".reportMenu"></activity>
    </application>

</manifest>